{
  "name": "Vk panel",
  "description": "Mini window with vk.com site inside",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "https://*.vk.com/",
    "http://*.vk.com/",
    "https://vk.com/",
    "http://vk.com/",
    "webview",
    "alwaysOnTopWindows",
    "storage"
  ],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "webview": {
    "partitions": [
      {
        "name": "static",
        "accessible_resources": ["vkcontent.js"]
      }
    ]
  },
  "icons": { "16": "icon.png", "128": "icon.png" }
}